console.log ("WELCOME TO JAVASCRIPT");
let a = 5;
let b = 2;
console.log ("types of operators");
console.log ("OPERATORS");
console.log("a + b =",a + b);
console.log("a - b =",a - b);
console.log("a / b =",a / b);
console.log("a % b =",a % b);
console.log ("ASSIGNMENT OPERATORS");
a += 4;
console.log("a = ",a );
a -= 4;
console.log("a = ",a );
a /= 4;
console.log("a = ",a );
a %= 4;
console.log("a = ",a );
a **= 4;
console.log("a = ",a );


